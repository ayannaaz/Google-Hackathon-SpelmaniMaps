#Make a marker for Albert E. Manley Student Center
studentCent = folium.RegularPolygonMarker(location = [33.7451, -84.4115], popup = 'Manley College Center', fill_color='#00BFFF',
    number_of_sides=4,
    radius=9).add_to(spelly)


